
@{
    ViewBag.Title = "ContactConfirmation";
}

<div class="mainContent">
    <div class="subContent">
        <div class="row">
            <div class="col-sm-12">
                <!-- response message, only visible if message is sent successfully. -->
                <h2 id="confirmText">
                    Thanks for contacting us, @Model.FirstName @Model.LastName! We will get back to you as soon as possible.
            </h2>
        </div>
    </div>
</div>

</div>

